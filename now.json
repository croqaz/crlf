{
    "version": 2,
    "routes": [
        {
            "src": "/(.+\\.woff2)$",
            "headers": {
                "Cache-Control": "s-maxage=604800, max-age=604800"
            }
        },
        {
            "src": "/(.+\\.(?:jpg|png|ico))$",
            "headers": {
                "Cache-Control": "s-maxage=604800, max-age=604800"
            }
        },
        {
            "src": "/favicon(.*\\.(?:png|ico))$",
            "headers": {
                "Cache-Control": "s-maxage=31536000, max-age=31536000, immutable",
                "X-Content-Type-Options": "no-sniff"
            }
        },
        {
            "handle": "filesystem"
        },
        {
            "src": "/(.*)",
            "dest": "/404.html",
            "status": 404
        }
    ]
}